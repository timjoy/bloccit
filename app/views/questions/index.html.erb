<h1>All questions</h1>

<div class="col-md-4">
  <%= link_to "Create a question.", new_question_path(@question), class: 'btn btn-success' %>
</div>

<% @questions.each do |question| %>
  <div class="media">
    <div class="media-body">
      <h4 class="media-heading">
        <%= link_to question.title, question %>
      </h4>
    </div>
  </div>
<% end %>
